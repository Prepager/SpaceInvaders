#include "Enemy.h"

/**
 * todo
 */
Enemy::Enemy() {
	//
	this->width = 640;
	this->height = 480;

	//
	this->xPos = 50;
	this->yPos = 50;
}

/**
 * todo
 */
void Enemy::render(u8 *frame, u32 x, u32 y, int addr) {
	// todo
	if ((x > this->xPos && x < this->xPos) && (y > this->yPos && y < this->yPos)) {
		frame[addr + 0] = 255;	// G
		frame[addr + 1] = 255;	// B
		frame[addr + 2] = 255;	// R
	}
}
